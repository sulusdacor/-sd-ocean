  <!-- rockchunks+rubble on land in ocean tiles -->
  
  <GenStepDef>
    <defName>sd_ocean_genstep_RockChunks</defName>
    <mapGenerator>BasicMapGenerator</mapGenerator>
    <order>233</order>
    <genStep Class="sd_ocean.GenStep_RockChunks_ocean"/>
  </GenStepDef>
  
