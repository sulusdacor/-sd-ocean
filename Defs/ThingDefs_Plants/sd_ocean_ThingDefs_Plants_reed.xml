<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!--======================= reed  ========================-->

  <ThingDef ParentName="PlantBase">
    <defName>sd_ocean_plant_reed</defName>
    <label>reed</label>
    <description>Wild tall reed. Significantly slows down anyone moving over it.</description>
    <statBases>
      <MaxHitPoints>90</MaxHitPoints>
      <Beauty>0</Beauty>
    </statBases>
    <pathCost>18</pathCost>
    <graphicData>
      <texPath>sd_ocean/plants/reed</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <hideAtSnowDepth>1</hideAtSnowDepth>
    <ingestible>
      <nutrition>0.15</nutrition>
    </ingestible>
	<selectable>true</selectable>
    <plant>
			<fertilityMin>0.05</fertilityMin>
			<fertilitySensitivity>1.0</fertilitySensitivity>
			<growDays>3</growDays>
			<reproduceMtbDays>3</reproduceMtbDays>
			<reproduceRadius>42</reproduceRadius>
			<harvestWork>60</harvestWork>
			<maxMeshCount>4</maxMeshCount>
			<visualSizeRange>
				<min>0.8</min>
				<max>0.95</max>
			</visualSizeRange>
			<topWindExposure>0.4</topWindExposure>
			<wildClusterSizeRange>
				<min>2</min>
				<max>5</max>
			</wildClusterSizeRange>
			<wildClusterRadius>4</wildClusterRadius>
			<wildCommonalityMaxFraction>3.0</wildCommonalityMaxFraction>
		</plant>
  </ThingDef>


</Defs>